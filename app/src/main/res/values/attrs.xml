<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <declare-styleable name="CustomButtonRadius">
        <attr name="text" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomCreditCard" tools:ignore="DuplicateDefinition">
        <attr name="textMetaValor" format="string"/>
        <attr name="textFaltaMetaValor" format="string"/>
        <attr name="textDiasUteis" format="string"/>
        <attr name="textDiasCorridos" format="string"/>
        <attr name="textPorcentIdeal" format="string"/>
        <attr name="textPorcentGrafic" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomViewHeader" tools:ignore="DuplicateDefinition">
        <attr name="textUdateHora" format="string"/>
        <attr name="textName" format="string"/>

    </declare-styleable>

    <declare-styleable name="CustomCreditCardLeao" tools:ignore="DuplicateDefinition">
        <attr name="textMetaValue" format="string"/>
