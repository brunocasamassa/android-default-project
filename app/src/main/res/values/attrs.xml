    <declare-styleable name="CustomCardViewScripts">
        <attr name="address" format="string"/>
        <attr name="time" format="string"/>
        <attr name="rota" format="string"/>
        <attr name="card_rota" format="color"/>
    </declare-styleable>

    <declare-styleable name="CustomCardViewTotalItensAndValue">
        <attr name="textItensCard" format="string"/>
        <attr name="textValueCard" format="string"/>
    </declare-styleable>

    <declare-styleable name="CustomCardViewDatail">
        <attr name="textImageCard" format="string"/>
        <attr name="textTitleCard" format="string"/>
        <attr name="textDescriptionCard" format="string"/>
        <attr name="textDateCard" format="string"/>
    </declare-styleable>


</resources>